{
  "name": "chad",
  "productName": "Chad",
  "version": "0.21.0",
  "description": "A chat client for Twitch",
  "main": "app/main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "pack": "build",
    "dist": "npm run dist:osx && npm run dist:win64",
    "dist:osx": "build --platform darwin",
    "dist:win64": "build --platform win32 --arch x64"
  },
  "keywords": [
    "chad",
    "chat",
    "twitch",
    "irc"
  ],
  "author": "D_4_ni",
  "license": "GPL-3.0",
  "devDependencies": {
    "electron": "^1.6.2",
    "electron-builder": "^15.6.2"
  },
  "dependencies": {
    "electron-remote": "^1.1.2"
  }
}
